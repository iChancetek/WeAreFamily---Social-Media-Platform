    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
